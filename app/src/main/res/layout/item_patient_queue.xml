                android:textColor="@android:color/white"
                android:textSize="@dimen/dp_25" />

            <TextView
                android:id="@+id/txt_family_members_names"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/txt_mobile_family"
                android:layout_below="@+id/txt_name_family"
                android:layout_toLeftOf="@+id/ibtn_view_details_family"
                android:ellipsize="end"
                android:gravity="top"
                android:maxLines="3"
                android:text=""
                android:textColor="@android:color/white"
                android:textSize="@dimen/dp_18" />


            <TextView
                android:id="@+id/txt_mobile_family"
                android:layout_width="@dimen/dp_170"
