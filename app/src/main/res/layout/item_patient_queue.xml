<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/lyt_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/rlt_container"
        android:visibility="visible">

        <RelativeLayout
            android:id="@+id/rlt_layout_patient_queue_register_user"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/dp_5"
            android:background="@drawable/parient_queue_row_bg"
            android:padding="@dimen/dp_10"
            android:visibility="visible">


            <ImageView
                android:id="@+id/img_serial_no_bg_register_user"
                android:layout_width="@dimen/dp_100"
                android:layout_height="@dimen/dp_120"
                android:layout_marginBottom="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_5"
                android:background="@drawable/number_count_icon" />

            <LinearLayout
                android:layout_width="@dimen/dp_100"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/img_serial_no_bg_register_user"
                android:layout_alignTop="@+id/img_serial_no_bg_register_user"
                android:layout_marginRight="@dimen/dp_5"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txt_serial_no_register_user"
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:shadowColor="@color/color_text_grey"
                    android:shadowDx="0"
                    android:shadowDy="1"
                    android:shadowRadius="1"
                    android:textColor="@color/faint_blue"
                    android:textSize="@dimen/dp_30"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/txt_appointment_time_register_user"
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:shadowColor="@color/color_text_grey"
                    android:shadowDx="0"
                    android:shadowDy="1"
                    android:shadowRadius="1"
                    android:textColor="@color/faint_blue"
                    android:textSize="@dimen/dp_14"
                    android:textStyle="bold" />
            </LinearLayout>


            <!-- <TextView
                 android:id="@+id/txt_serial_no_register_user"
                 android:layout_width="@dimen/dp_100"
                 android:layout_height="@dimen/dp_120"
                 android:layout_marginBottom="@dimen/dp_10"
                 android:layout_marginRight="@dimen/dp_5"
                 android:background="@drawable/number_count_icon"
                 android:gravity="center"
                 android:shadowColor="@color/color_text_grey"
                 android:shadowDx="0"
                 android:shadowDy="1"
                 android:shadowRadius="1"
                 android:textColor="@color/faint_blue"
                 android:textSize="@dimen/dp_30"
                 android:textStyle="bold" />-->

            <com.dhb.customview.RoundedCornerRectangleImageView
                android:id="@+id/img_profile_pic_register_user"
                android:layout_width="@dimen/dp_100"
                android:layout_height="@dimen/dp_100"
                android:layout_alignTop="@+id/img_serial_no_bg_register_user"
                android:layout_marginBottom="@dimen/dp_10"
                android:layout_marginLeft="@dimen/dp_5"
                android:layout_marginRight="@dimen/dp_10"
                android:layout_toRightOf="@+id/img_serial_no_bg_register_user"
                android:gravity="center"
                android:scaleType="centerCrop" />

            <TextView
                android:id="@+id/txt_name_register_user"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/img_serial_no_bg_register_user"
                android:layout_marginLeft="@dimen/dp_5"
                android:layout_marginTop="-5dp"
                android:layout_toLeftOf="@+id/ibtn_view_details_register_user"
                android:layout_toRightOf="@+id/img_profile_pic_register_user"
                android:ellipsize="end"
                android:gravity="top"
                android:singleLine="true"
                android:textColor="@color/text_color"
                android:textSize="@dimen/dp_25" />

            <TextView
                android:id="@+id/txt_age_register_user"
                android:layout_width="@dimen/dp_170"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_name_register_user"
                android:layout_toRightOf="@+id/img_profile_pic_register_user"
                android:drawableLeft="@drawable/profile_icon_black"
                android:drawablePadding="@dimen/dp_10"
                android:textColor="@color/text_color"
                android:textSize="@dimen/dp_18" />

            <TextView
                android:id="@+id/txt_gender_register_user"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_name_register_user"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_toRightOf="@+id/txt_age_register_user"
                android:textColor="@color/text_color"
                android:textSize="@dimen/dp_18" />

            <TextView
                android:id="@+id/txt_mobile_register_user"
                android:layout_width="@dimen/dp_170"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_age_register_user"
                android:layout_toRightOf="@+id/img_profile_pic_register_user"
                android:drawableLeft="@drawable/phone_icon1_black"
                android:drawablePadding="@dimen/dp_10"
                android:textColor="@color/text_color"
                android:textSize="@dimen/dp_18" />

            <TextView
                android:id="@+id/txt_time_register_user"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_age_register_user"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_toRightOf="@+id/txt_mobile_register_user"
                android:drawableLeft="@drawable/clock_icon_black"
                android:drawablePadding="@dimen/dp_10"
                android:textColor="@color/text_color"
                android:textSize="@dimen/dp_18" />


            <TextView
                android:id="@+id/txt_blood_group_register_user"
                android:layout_width="@dimen/dp_70"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_mobile_register_user"
                android:layout_toRightOf="@+id/img_profile_pic_register_user"
                android:drawableLeft="@drawable/blood_group_icon_red"
                android:drawablePadding="@dimen/dp_10"
                android:textColor="@color/text_color"
                android:textSize="@dimen/dp_18" />

            <TextView
                android:id="@+id/txt_visit_reason_register_user"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_mobile_register_user"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_toRightOf="@+id/txt_blood_group_register_user"
                android:drawableLeft="@drawable/complaint_icon_black"
                android:drawablePadding="@dimen/dp_10"
                android:textColor="@color/text_color"
                android:textSize="@dimen/dp_18" />
            />

            <TextView
                android:id="@+id/txt_chief_complaint_register_user"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_HIN_register_user"
                android:layout_toRightOf="@+id/img_profile_pic_register_user"
                android:singleLine="true"
                android:text=""
                android:textColor="@color/text_color"
                android:textSize="@dimen/dp_18" />

            <TextView
                android:id="@+id/txt_HIN_register_user"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_blood_group_register_user"
                android:layout_toRightOf="@+id/img_profile_pic_register_user"
                android:text="HIN: "
                android:textColor="@color/text_color"
                android:textSize="@dimen/dp_18" />

            <TextView
                android:id="@+id/txt_hospital_id_register_user"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_chief_complaint_register_user"
                android:layout_toRightOf="@+id/img_profile_pic_register_user"
                android:text="Hospital ID - "
                android:textColor="@color/text_color"
                android:textSize="@dimen/dp_18"
                android:visibility="invisible" />

            <ImageButton
                android:id="@+id/ibtn_view_details_register_user"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@+id/img_serial_no_bg_register_user"
                android:background="@android:color/transparent"
                android:paddingBottom="@dimen/dp_20"
                android:paddingLeft="@dimen/dp_30"
                android:paddingRight="@dimen/dp_5"
                android:paddingTop="@dimen/dp_2"
                android:src="@drawable/selector_patient_ribbon_menu" />


        </RelativeLayout>

