                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_name_mr"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_toRightOf="@+id/txt_age_mr"
                android:textColor="@android:color/white"
                android:textSize="@dimen/dp_18" />


            <TextView
                android:id="@+id/txt_mobile_mr"
                android:layout_width="@dimen/dp_170"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_age_mr"
                android:layout_toRightOf="@+id/img_profile_pic_mr"
                android:drawableLeft="@drawable/phone_icon_white"
                android:drawablePadding="@dimen/dp_10"
                android:textColor="@android:color/white"
                android:textSize="@dimen/dp_18" />

            <TextView
                android:id="@+id/txt_time_mr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_gender_mr"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_toRightOf="@+id/txt_mobile_mr"
                android:drawableLeft="@drawable/clock_icon_white"
                android:drawablePadding="@dimen/dp_10"
                android:textColor="@android:color/white"
                android:textSize="@dimen/dp_18" />


            <TextView
                android:id="@+id/txt_blood_group_mr"
                android:layout_width="@dimen/dp_70"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_mobile_mr"
                android:layout_toRightOf="@+id/img_profile_pic_mr"
                android:drawableLeft="@drawable/blood_group_icon_red"
                android:drawablePadding="@dimen/dp_10"
                android:textColor="@android:color/white"
                android:textSize="@dimen/dp_18" />

            <TextView
                android:id="@+id/txt_visit_reason_mr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_mobile_mr"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_toRightOf="@+id/txt_blood_group_mr"
                android:drawableLeft="@drawable/clock_icon_white"
                android:drawablePadding="@dimen/dp_10"
                android:textColor="@android:color/white"
                android:textSize="@dimen/dp_18"
                android:visibility="invisible" />
            />

            <TextView
                android:id="@+id/txt_chief_complaint_mr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_HIN_mr"
                android:layout_toRightOf="@+id/img_profile_pic_mr"
                android:singleLine="true"
                android:text=""
                android:textColor="@android:color/white"
                android:textSize="@dimen/dp_18" />

            <TextView
                android:id="@+id/txt_HIN_mr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_blood_group_mr"
                android:layout_toRightOf="@+id/img_profile_pic_mr"
                android:text="HIN: "
                android:textColor="@android:color/white"
                android:textSize="@dimen/dp_18" />


            <TextView
                android:id="@+id/txt_hospital_id_mr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_chief_complaint_mr"
                android:layout_toRightOf="@+id/img_profile_pic_mr"
                android:text="Hospital ID - "
                android:textColor="@color/text_color"
                android:textSize="@dimen/dp_18"
                android:visibility="gone" />

            <TextView
                android:id="@+id/txt_blank_mr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_hospital_id_mr"
                android:layout_toRightOf="@+id/img_profile_pic_mr"
                android:text="HIN : "
                android:textColor="@android:color/white"
                android:textSize="@dimen/dp_18"
                android:visibility="invisible" />


            <ImageButton
                android:id="@+id/ibtn_view_details_mr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@+id/img_serial_no_bg_mr"
                android:background="@android:color/transparent"
                android:paddingLeft="@dimen/dp_30"
                android:paddingRight="@dimen/dp_5"
                android:paddingTop="@dimen/dp_2"
                android:src="@drawable/selector_patient_ribbon_menu_white" />


        </RelativeLayout>

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_5"
        android:background="@color/patient_queue_red_color"
        android:orientation="horizontal"
        android:visibility="gone"
        android:weightSum="3">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/txt_delete"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_20"
                android:clickable="false"
                android:gravity="center_vertical"
                android:hapticFeedbackEnabled="true"
                android:text="@string/no_show"
                android:textColor="@android:color/white"
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txt_undo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dp_20"
                android:clickable="false"
                android:gravity="center"
                android:text="@string/undo"
                android:textColor="@android:color/white"
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txt_delete_hindi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/txt_delete"
                android:layout_marginLeft="@dimen/dp_20"
                android:clickable="false"
                android:gravity="center_vertical"
                android:hapticFeedbackEnabled="true"
                android:text="@string/no_show_hindi"
                android:textColor="@android:color/white"
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txt_undo_hindi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/txt_undo"
                android:layout_marginRight="@dimen/dp_20"
                android:clickable="false"
                android:gravity="center"
                android:text="@string/undo_hindi"
                android:textColor="@android:color/white"
                android:textSize="20dp"
                android:textStyle="bold" />


        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_5"
        android:background="@color/patient_queue_green_color"
        android:orientation="horizontal"
        android:visibility="gone"
        android:weightSum="3">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/txt_action"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_20"
                android:clickable="false"
                android:gravity="center_vertical"
                android:hapticFeedbackEnabled="true"
                android:text="@string/send_to_doctor"
                android:textColor="@android:color/white"
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txt_undo_action"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dp_20"
                android:layout_weight="1"
                android:clickable="false"
                android:gravity="center"
                android:text="@string/undo"
                android:textColor="@android:color/white"
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txt_action_hindi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/txt_action"
                android:layout_marginLeft="@dimen/dp_20"
                android:clickable="false"
                android:gravity="center_vertical"
                android:hapticFeedbackEnabled="true"
                android:text="@string/send_to_doctor_hindi"
                android:textColor="@android:color/white"
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txt_undo_action_hindi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/txt_action"
                android:layout_marginRight="@dimen/dp_20"
                android:layout_weight="1"
                android:clickable="false"
                android:gravity="center"
                android:text="@string/undo_hindi"
                android:textColor="@android:color/white"
                android:textSize="20dp"
                android:textStyle="bold" />
        </RelativeLayout>
    </LinearLayout>
</FrameLayout>
