            <TextView
                android:id="@+id/txt_HIN_unregister_user"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txt_blood_group_unregister_user"
                android:layout_toRightOf="@+id/img_profile_pic_unregister_user"
